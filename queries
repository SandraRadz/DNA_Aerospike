# delete rows
TRUNCATE test.fasta

CREATE INDEX fastaName ON test.fasta (name) STRING

DROP INDEX test name

SET OUTPUT JSON

register module 'custom_functions.lua'

SELECT * FROM test.fasta WHERE PK=""

EXECUTE custom_functions.diff_num() ON test.fasta WHERE PK=""

AGGREGATE custom_functions.total_diff_num() ON test.fasta

AGGREGATE custom_functions.wild_type() ON test.fasta